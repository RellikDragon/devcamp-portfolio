<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], :title => "Jared Dunn's Portfolio", sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], :title => "Jared Dunn's Portfolio", sticky: false) %>
<% else %>
  <%= js add_gritter(flash[:notice], :title => "Jared Dunn's Portfolio", sticky: false) %> 
<% end %> 